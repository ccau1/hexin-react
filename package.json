{
  "name": "hexin-react",
  "version": "0.1.3",
  "description": "HEXIN Framework for React-Native",
  "main": "src/index.js",
  "scripts": {
    "test": "cross-env NODE_PATH=./src NODE_ENV=testing mocha \"__test__/**/*.js\" --compilers js:babel-register",
    "build": "webpack -p --progress --colors",
    "buildProd": "cross-env NODE_ENV=production npm run build",
    "buildTest": "cross-env NODE_ENV=testing npm run build",
    "start": "cross-env NODE_ENV=development && node --inspect server",
    "startTest": "cross-env NODE_ENV=testing node server",
    "startProd": "cross-env NODE_ENV=production node server",
    "bstartTest": "npm run build | npm run startTest",
    "bstartProd": "npm run build | npm run startProd",
    "lint": "eslint src --ext .jsx,.js || true",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "start-hot": "webpack-dev-server --port 3000 --watch --hot --watch-poll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccau1/hexin-react.git"
