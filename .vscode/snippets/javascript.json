      ""
    ]
  },

  /* STYLES */

  "Styled Component": {
    "prefix": "styd",
    "body": [
      "import styled from \"styled-components\";",
      "",
      "export default styled.div`",
      "  $0",
      "`;"
    ]
  },

  "Stylesheet page": {
    "prefix": "stylesheet",
    "body": [
      "/* @flow */",
      "",
      "module.exports = require('react-native').StyleSheet.create({",
      "  $1",
      "});"
    ]
  },

  /* MESSAGES */

  "Intl Messages": {
    "prefix": "imsg",
    "body": [
      "/* @flow */",
      "",
      "import {defineMessages} from 'react-intl';",
      "",
      "export default defineMessages({",
      "  $1",
      "});"
    ]
  }
}
